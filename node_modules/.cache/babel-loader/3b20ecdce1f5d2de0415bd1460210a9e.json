tUser.title }}\n        </div>\n        <div>\n          <label><strong>Description:</strong></label>\n          {{ currentUser.description }}\n        </div>\n\n        <router-view></router-view>\n        <a\n          class=\"badge badge-warning\"\n          :href=\"'/user/' + currentExample.id\"\n        >\n          Edit\n        </a>\n      </div>\n      <div v-else>\n        <br />\n        <p>Please click on a Example...</p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script lang='ts'>\nimport { defineComponent } from 'vue';\nimport User from '@/model/user';\nimport UserCom from './User.vue';\nimport AddUser from './AddUser.vue';\n\nexport default defineComponent({\n  name: 'UserList',\n  data() {\n    return {\n      users: [new User('Martin', 'description of your choice')], // [] as Array<User>,\n      currentIndex: -1,\n    };\n  },\n  methods: {\n    addUser(user: User): void {\n      user.id = this.users.length; // eslint-disable-line no-param-reassign\n      this.users.push(user);\n    },\n    setActiveUser(index: number): void {\n      this.currentIndex = -1;\n    },\n    removeUser(index: number): void {\n      if (index > -1) {\n        this.users.splice(index, 1);\n      }\n      /* eslint no-plusplus: [\"error\", { \"allowForLoopAfterthoughts\": true }] */\n\n      for (index = 0; index < this.users.length; index++) { // eslint-disable-line no-param-reassign\n        this.users[index].id = index; // eslint-disable-line no-param-reassign\n      }\n      this.currentIndex = -1;\n    },\n    removeAllUsers(): void {\n      this.currentIndex = -1;\n      this.users = [] as Array<User>;\n    },\n  },\n  computed: {\n    currentUser(): (User|null) {\n      if (this.currentIndex < 0) {\n        return null;\n      }\n      return this.users[this.currentIndex];\n    },\n  },\n  components: {\n    UserCom, // eslint-disable-line vue/no-unused-components\n    AddUser, // eslint-disable-line vue/no-unused-components\n  },\n});\n\n</script>\n\n<style scoped>\n.list {\n  text-align: left;\n  max-width: 750px;\n  margin: auto;\n}\n</style>\n"],"sourceRoot":""}]}