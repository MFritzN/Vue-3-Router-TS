{"remainingRequest":"/home/neuner/my-project/node_modules/vue-loader-v16/dist/index.js??ref--0-1!/home/neuner/my-project/src/components/Example.vue?vue&type=style&index=0&id=220f7e6a&scoped=true&lang=css","dependencies":[{"path":"/home/neuner/my-project/src/components/Example.vue","mtime":1597046107732},{"path":"/home/neuner/my-project/node_modules/css-loader/dist/cjs.js","mtime":499162500000},{"path":"/home/neuner/my-project/node_modules/vue-loader-v16/dist/stylePostLoader.js","mtime":499162500000},{"path":"/home/neuner/my-project/node_modules/postcss-loader/src/index.js","mtime":499162500000},{"path":"/home/neuner/my-project/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/home/neuner/my-project/node_modules/vue-loader-v16/dist/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:Ci5lZGl0LWZvcm0gewogIG1heC13aWR0aDogMzAwcHg7CiAgbWFyZ2luOiBhdXRvOwp9Cg=="},{"version":3,"sources":["/home/neuner/my-project/src/components/Example.vue"],"names":[],"mappings":";AAiIA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;EACT,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC;EAChB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;AACd","file":"/home/neuner/my-project/src/components/Example.vue","sourceRoot":"","sourcesContent":["<template>\n  <div v-if='currentExample' class='edit-form'>\n    <h4>Example</h4>\n    <form>\n      <div class='form-group'>\n        <label for='title'>Title</label>\n        <input\n          type='text'\n          class='form-control'\n          id='title'\n          v-model='currentExample.title'\n        />\n      </div>\n      <div class='form-group'>\n        <label for='description'>Description</label>\n        <input\n          type='text'\n          class='form-control'\n          id='description'\n          v-model='currentExample.description'\n        />\n      </div>\n\n      <div class='form-group'>\n        <label><strong>Status:</strong></label>\n        {{ currentExample.published ? 'Published' : 'Pending' }}\n      </div>\n    </form>\n\n    <button\n      class='badge badge-primary mr-2'\n      v-if='currentExample.published'\n      @click='updatePublished(false)'\n    >\n      UnPublish\n    </button>\n    <button\n      v-else\n      class='badge badge-primary mr-2'\n      @click='updatePublished(true)'\n    >\n      Publish\n    </button>\n\n    <button class='badge badge-danger mr-2' @click='deleteExample'>\n      Delete\n    </button>\n\n    <button type='submit' class='badge badge-success' @click='updateExample'>\n      Update\n    </button>\n    <p>{{ message }}</p>\n  </div>\n\n  <div v-else>\n    <br />\n    <p>Please click on a Example...</p>\n  </div>\n</template>\n\n<script lang='ts'>\nimport { Component, Vue } from 'vue-property-decorator';\nimport ExampleDataService from '../services/ExampleDataService';\n\n@Component\nexport default class Example extends Vue {\n  private currentExample: any = null;\n\n  private message: string = '';\n\n  getExample(id: string) {\n    ExampleDataService.get(id)\n      .then((response) => {\n        this.currentExample = response.data;\n        console.log(response.data);\n      })\n      .catch((e) => {\n        console.log(e);\n      });\n  }\n\n  updatePublished(status: boolean) {\n    var data = {\n      id: this.currentExample.id,\n      title: this.currentExample.title,\n      description: this.currentExample.description,\n      published: status,\n    };\n\n    ExampleDataService.update(this.currentExample.id, data)\n      .then((response) => {\n        this.currentExample.published = status;\n        console.log(response.data);\n      })\n      .catch((e) => {\n        console.log(e);\n      });\n  }\n\n  updateExample() {\n    ExampleDataService.update(this.currentExample.id, this.currentExample)\n      .then((response) => {\n        console.log(response.data);\n        this.message = 'The example was updated successfully!';\n      })\n      .catch((e) => {\n        console.log(e);\n      });\n  }\n\n  deleteExample() {\n    ExampleDataService.delete(this.currentExample.id)\n      .then((response) => {\n        console.log(response.data);\n        this.$router.push({ name: 'examples' });\n      })\n      .catch((e) => {\n        console.log(e);\n      });\n  }\n\n  mounted() {\n    this.message = '';\n    this.getExample(this.$route.params.id);\n  }\n}\n</script>\n\n<style scoped>\n.edit-form {\n  max-width: 300px;\n  margin: auto;\n}\n</style>\n"]}]}