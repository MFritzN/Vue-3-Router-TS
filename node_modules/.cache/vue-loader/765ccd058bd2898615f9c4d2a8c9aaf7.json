\">\n      <h4>Examples List</h4>\n      <ul class=\"list-group\">\n        <li\n          class=\"list-group-item\"\n          :class=\"{ active: index == currentIndex }\"\n          v-for=\"(txample, index) in examples\"\n          :key=\"index\"\n          @click=\"setActiveExample(example, index)\"\n        >\n          {{ example.title }}\n        </li>\n      </ul>\n\n      <button class=\"m-3 btn btn-sm btn-danger\" @click=\"removeAllExamples\">\n        Remove All\n      </button>\n    </div>\n    <div class=\"col-md-6\">\n      <div v-if=\"currentExample\">\n        <h4>Example</h4>\n        <div>\n          <label><strong>Title:</strong></label> {{ currentExample.title }}\n        </div>\n        <div>\n          <label><strong>Description:</strong></label>\n          {{ currentExample.description }}\n        </div>\n        <div>\n          <label><strong>Status:</strong></label>\n          {{ currentExample.published ? \"Published\" : \"Pending\" }}\n        </div>\n\n        <a\n          class=\"badge badge-warning\"\n          :href=\"'/examples/' + currentExample.id\"\n        >\n          Edit\n        </a>\n      </div>\n      <div v-else>\n        <br />\n        <p>Please click on a Example...</p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script lang='ts'>\nimport { Component, Vue } from 'vue-property-decorator';\nimport ExampleDataService from '../services/ExampleDataService';\n\n@Component\nexport default class ExamplesList extends Vue {\n  private examples: any[] = [];\n\n  private currentExample: any = null;\n\n  private currentIndex = -1;\n\n  private title = '';\n\n  retrieveExamples() {\n    ExampleDataService.getAll()\n      .then((response) => {\n        this.examples = response.data;\n        console.log(response.data);\n      })\n      .catch((e) => {\n        console.log(e);\n      });\n  }\n\n  refreshList() {\n    this.retrieveExamples();\n    this.currentExample = null;\n    this.currentIndex = -1;\n  }\n\n  setActiveExample(example: any, index: number) {\n    this.currentExample = example;\n    this.currentIndex = index;\n  }\n\n  removeAllExamples() {\n    ExampleDataService.deleteAll()\n      .then((response) => {\n        console.log(response.data);\n        this.refreshList();\n      })\n      .catch((e) => {\n        console.log(e);\n      });\n  }\n\n  searchTitle() {\n    ExampleDataService.findByTitle(this.title)\n      .then((response) => {\n        this.examples = response.data;\n        console.log(response.data);\n      })\n      .catch((e) => {\n        console.log(e);\n      });\n  }\n\n  mounted() {\n    this.retrieveExamples();\n  }\n}\n</script>\n\n<style scoped>\n.list {\n  text-align: left;\n  max-width: 750px;\n  margin: auto;\n}\n</style>\n"]}]}